{
    "name": "forci/remember-me-bundle",
    "type": "symfony-bundle",
    "description": "Persistent Remember Me bundle for Symfony applications",
    "keywords": ["symfony remember me", "remember me", "symfony", "wucdbm"],
    "authors": [
        {
            "name": "Martin Kirilov",
            "email": "wucdbm@gmail.com",
            "role": "Developer"
        },
        {
            "name": "Tatjana Mincheva",
            "email": "tatjana@forci.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "symfony/framework-bundle": "~2.0|~3.0|~4.0",
        "symfony/security": "~2.0|~3.0|~4.0",
        "symfony/form": "~2.0|~3.0|~4.0",
        "doctrine/orm": "~2.5",
        "wucdbm/quick-ui-bundle": "~0.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.8",
        "wucdbm/php-cs-fixers": "~0.1"
    },
    "autoload": {
        "psr-4": { "Forci\\Bundle\\RememberMeBundle\\": "src" }
    },
    "config": {
        "bin-dir": "bin"
    },
